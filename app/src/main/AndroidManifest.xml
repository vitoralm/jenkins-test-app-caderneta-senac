        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AtvHome"></activity>
        <activity android:name=".AtvAlterarRenda" />
        <activity android:name=".AtvVerGastos"/>
        <activity android:name=".AtvAdicionarGasto" />
        <activity android:name=".AtvCriarConta" />
        <activity android:name=".AtvLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest>